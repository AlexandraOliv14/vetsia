<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
<h1>Due√±os</h1>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Nombre</th>
            <th>Telefono</th>
            <th >Mail</th>
            <th>Mostrar</th>
            <th>Editar</th>
            <th>Eliminar</th>
        </tr>
    </thead>
    <tbody>
        <% @duenos.each do |dueno| %>
          <tr>
              <td><%= dueno.nombre %></td>
              <td><%= dueno.telefono %></td>
              <td><%= dueno.mail %><br></td>
              <td><%= link_to 'Mostrar', dueno,class: 'btn btn-success ' %> </td>
              <td><%= link_to 'Editar',editar_dueno_path(dueno),class: 'btn btn-warning '  %></td>
              <td><%= link_to'Eliminar',dueno,class: 'btn btn-danger ' , method: :delete, data: {confirm: 'Esta seguro?'} %></td>
          </tr>
        <% end %>
    </tbody>
</table>
<%= will_paginate @duenos, renderer: BootstrapPagination::Rails %>
